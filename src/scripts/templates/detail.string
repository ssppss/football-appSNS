<div class="detail">
	<header>
		<ul>
			<li action-type="goto.back" class="yo-ico menu1">&#xe600;</li>
			<li>{{title}}</li>
			<li class="yo-ico sca1">&#xe609;</li>
		</ul>
	</header>
	<section ms-visible="!isShowLoading">
		<p><img ms-src="imgsrc"></p>
		<article>
			{{description}}
		</article>
	</section>
	<div class="yo-loading" ms-visible="isShowLoading">
		<div class="inner"><i class="yo-ico"></i></div>
	</div>
</div>